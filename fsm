digraph "State Machine" {
	graph [color=black directed=true fillcolor=white label="State Machine" rankdir=LR "strict"=false style=solid]
	node [color=black fillcolor=white peripheries=1 shape=rectangle style="rounded, filled"]
	edge [color=black]
	user [label=user color=red fillcolor=darksalmon peripheries=2]
	op1 [label=op1]
	run [label=run]
	help [label=help]
	fsm [label=fsm]
	translate1 [label=translate1]
	translate2 [label=translate2]
	translate3 [label=translate3]
	user -> op1 [label="advance [sing_op1]"]
	user -> run [label="advance [sing_run]"]
	user -> help [label="advance [help_me]"]
	user -> fsm [label="advance [show_fsm]"]
	user -> translate1 [label="advance [go_to_translate1]"]
	translate1 -> translate2 [label="advance [go_to_translate2]"]
	translate2 -> translate3 [label="advance [go_to_translate3]"]
	translate2 -> user [label=go_back]
	translate3 -> translate1 [label="advance [return_translate1] | return_translate1"]
	translate3 -> user [label=go_back]
	translate3 -> translate2 [label=return_translate2]
	op1 -> user [label=go_back]
	run -> user [label=go_back]
	help -> user [label=go_back]
	fsm -> user [label=go_back]
}
